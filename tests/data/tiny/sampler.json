{
  "kernels": {
    "args": [
      "__global float*",
      "__global float*",
      "__global float*",
      "const int"
    ],
    "max_length": 500,  # max kernel length
    "temperature": 1  # sampling temperature
  },
  "sampler": {
    "min_kernels": 100,  # if <= 0, no minimum
    "min_samples": -1,  # if <= 0, no minimum
    "static_checker": true,  # {true,false}
    "dynamic_checker": false,  # {true,false}
    "gpuverify": false  # {true,false}
  }
}
